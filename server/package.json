{
  "name": "xui-proxy-server",
  "version": "1.0.0",
  "description": "Minimal webhook proxy server for n8n integration",
  "type": "module",
  "main": "n8n-webhook-proxy.js",
  "scripts": {
    "start": "node n8n-webhook-proxy.js",
    "dev": "node --watch n8n-webhook-proxy.js",
    "pm2:start": "pm2 start n8n-webhook-proxy.js --name n8n-webhook-proxy",
    "pm2:stop": "pm2 stop n8n-webhook-proxy",
    "pm2:restart": "pm2 restart n8n-webhook-proxy",
    "payment:start": "node payment-server.js",
    "payment:dev": "node --watch payment-server.js",
    "payment:pm2": "pm2 start payment-server.js --name payment-server"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase-admin": "^12.7.0",
    "helmet": "^7.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "proxy",
    "n8n",
    "webhook",
    "api"
  ],
  "author": "",
  "license": "ISC"
}
